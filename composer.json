{
	"name": "edwardkarlsson/laravel-linguini",
	"description": "Multi Language CMS addon for Laravel",
	"type": "library",
	"keywords": ["laravel", "multi-language", "content"],,
	"license": "MIT",
	"authors": [
		{
			"name": "Edward Karlsson",
			"email": "edward@medinaproduction.se"
		}
	],
	"require": {
		"php": ">=7.0",
		"laravel/framework": "~5.4",
		"fzaninotto/faker": "~1.7"
	},
	"require-dev": {
		"mockery/mockery": "^0.9.5",
		"phpunit/phpunit": "~7.0"
	},
	"autoload": {
		"psr-4": {
			"Linguini\\": "src/",
			"Linguini\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Linguini\\ServiceProvider"
			]
		}
	}
}
